{
  "name": "@economic-control/server",
  "version": "1.0.0",
  "description": "economic control server",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "db:migrate": "NODE_OPTIONS='--loader ts-node/esm --no-warnings' sequelize-cli db:migrate",
    "db:seed": "NODE_OPTIONS='--loader ts-node/esm --no-warnings' ts-node src/seeders/initDatabases.ts",
    "db:setup": "pnpm db:migrate && pnpm db:seed",
    "start": "node src/app.ts",
    "dev": "tsx watch --clear-screen=false src/app.ts",
    "dev:debug": "tsx watch --inspect src/app.ts",
    "build": "tsc"
  },
  "keywords": [],
  "author": "leodev",
  "license": "ISC",
  "packageManager": "pnpm@10.26.2+sha512.0e308ff2005fc7410366f154f625f6631ab2b16b1d2e70238444dd6ae9d630a8482d92a451144debc492416896ed16f7b114a86ec68b8404b2443869e68ffda6",
  "dependencies": {
    "@economic-control/shared": "workspace:*",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "mysql2": "^3.16.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "postgres": "^3.4.7",
    "sequelize": "^6.37.7",
    "socket.io": "^4.8.3",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.4",
    "nodemon": "^3.1.11",
    "sequelize-cli": "^6.6.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
