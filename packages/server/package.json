{
  "name": "@economic-control/server",
  "version": "1.2.0",
  "description": "economic control server",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist .turbo node_modules tsconfig.tsbuildinfo",
    "dev": "tsx watch --clear-screen=false src/app.ts",
    "lint": "eslint src",
    "build": "tsc -b && tsc-alias && copyfiles -u 1 \"src/migrations/**/*.cjs\" dist/",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed:dev": "tsx src/seeders/initDatabases.ts",
    "db:seed:prod": "node dist/seeders/initDatabases.js",
    "db:setup": "pnpm db:migrate && pnpm db:seed:dev",
    "db:setup:prod": "pnpm db:migrate && pnpm db:seed:prod",
    "db:deploy": "npx sequelize-cli db:migrate --env production && node dist/seeders/initDatabases.js",
    "start:prod": "node dist/app.js"
  },
  "keywords": [],
  "author": "leodev",
  "license": "ISC",
  "packageManager": "pnpm@10.30.0+sha512.2b5753de015d480eeb88f5b5b61e0051f05b4301808a82ec8b840c9d2adf7748eb352c83f5c1593ca703ff1017295bc3fdd3119abb9686efc96b9fcb18200937",
  "dependencies": {
    "@economic-control/shared": "workspace:*",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "mysql2": "^3.16.0",
    "nodemailer": "^8.0.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "postgres": "^3.4.7",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "socket.io": "^4.8.3",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.4",
    "@types/nodemailer": "^7.0.10",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
