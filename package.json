{
  "name": "economic-control-root",
  "version": "0.0.1",
  "private": true,
  "description": "Configuration root package for Economic Control monorepo",
  "scripts": {
    "install:all": "pnpm install",
    "db:setup": "turbo run db:setup --filter=@economic-control/server",
    "init": "pnpm install && pnpm build && pnpm db:setup && pnpm dev",
    "dev": "turbo dev",
    "build": "turbo build",
    "clean": "turbo clean && rm -rf node_modules .turbo packages/**/node_modules",
    "lint": "turbo lint",
    "lint:fix": "turbo lint -- --fix"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "sequelize-cli": "^6.6.5",
    "turbo": "^2.8.3",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.29.2+sha512.bef43fa759d91fd2da4b319a5a0d13ef7a45bb985a3d7342058470f9d2051a3ba8674e629672654686ef9443ad13a82da2beb9eeb3e0221c87b8154fff9d74b8"
}
