{
  "name": "economic-control-root",
  "version": "0.0.1",
  "private": true,
  "description": "Configuration root package for Economic Control monorepo",
  "scripts": {
    "install:all": "pnpm install",
    "db:setup": "turbo run db:setup --filter=@economic-control/server",
    "initial": "pnpm install && turbo run build && pnpm db:setup && pnpm dev",
    "dev": "turbo dev",
    "build": "turbo build",
    "clean": "turbo clean && rm -rf node_modules .turbo packages/**/node_modules",
    "lint": "turbo lint",
    "lint:fix": "turbo lint -- --fix"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "sequelize-cli": "^6.6.5",
    "turbo": "^2.8.10",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.30.0+sha512.2b5753de015d480eeb88f5b5b61e0051f05b4301808a82ec8b840c9d2adf7748eb352c83f5c1593ca703ff1017295bc3fdd3119abb9686efc96b9fcb18200937"
}
