{
  "name": "economic-control-root",
  "version": "0.0.0",
  "private": true,
  "description": "Configuration root package for Economic Control monorepo",
  "scripts": {
    "install:all": "pnpm install",
    "db:setup": "turbo db:setup",
    "init": "pnpm install:all && pnpm db:setup && pnpm dev",
    "dev": "turbo dev",
    "build": "turbo build",
    "clean": "turbo clean && rm -rf node_modules .turbo packages/**/node_modules",
    "lint": "turbo lint",
    "lint:fix": "turbo lint -- --fix"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "turbo": "^2.8.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.28.2+sha512.41872f037ad22f7348e3b1debbaf7e867cfd448f2726d9cf74c08f19507c31d2c8e7a11525b983febc2df640b5438dee6023ebb1f84ed43cc2d654d2bc326264"
}
